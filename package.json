{
  "name": "magnascale",
  "version": "2.0.0",
  "private": true,
  "description": "Lenovo MagnaScale Manager",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env PROGRESS=none umi build",
    "build-hg": "node hgbuild/index.js && npm run build",
    "dev": "npm run start:dev",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "openapi": "npm run openapi:doc && node doc/generate.js",
    "openapi:doc": "swagger-cli bundle -o doc/swagger.json doc/swagger.yaml",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "tsc": "tsc --noEmit",
    "test": "umi test"
  },
  "precommit": "lint-staged",
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 Edge versions"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-card": "^1.13.2",
    "@ant-design/pro-descriptions": "^1.6.8",
    "@ant-design/pro-form": "^1.18.3",
    "@ant-design/pro-layout": "^6.15.3",
    "@ant-design/pro-table": "^2.30.8",
    "@antv/xflow": "^1.0.33",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@umijs/route-utils": "^1.0.36",
    "ahooks": "^2.10.9",
    "antd": ">=4.17.4",
    "classnames": "^2.2.6",
    "echarts": "^5.2.1",
    "echarts-for-react": "^3.0.1",
    "echarts-liquidfill": "^3.1.0",
    "file-saver": "^2.0.5",
    "js-base64": "^3.7.5",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "omit.js": "^2.0.2",
    "rc-cascader": "^3.6.1",
    "react": "^17.0.0",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-virtualized": "^9.22.3",
    "umi": "^3.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.2",
    "@types/lodash": "^4.14.144",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.6.2",
    "@umijs/preset-react": "^1.8.17",
    "camelcase": "^6.2.0",
    "cross-env": "^7.0.0",
    "eslint": "^7.1.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "openapi-ln": "^1.0.3",
    "patch-package": "^6.4.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "stylelint": "^13.0.0",
    "swagger-cli": "^4.0.4",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
